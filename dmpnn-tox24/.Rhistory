df <- read.csv("test_preds.csv")
head(df)
reverse_min_max <- function(scaledValue, originalMin, originalMax){
  originalValue <- (scaledValue * (originalMax - originalMin)) + originalMin
  return(originalValue)

}
df$AR_true <- sapply(df$AR_true, reverse_min_max, originalMin = -45.00, originalMax = 111.10)
df$AR <- sapply(df$AR, reverse_min_max, originalMin = -45.00, originalMax = 111.10)
write.csv(df, "reversed_data_frame.csv")
q()
